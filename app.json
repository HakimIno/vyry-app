{
  "expo": {
    "name": "vyry",
    "slug": "vyry",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "vyry",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kimsnow33.vyry"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.kimsnow33.vyry"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true,
            "extraGradleProperties": [
              {
                "name": "org.gradle.jvmargs",
                "value": "-Xmx6144m -XX:MaxMetaspaceSize=1024m -XX:+UseParallelGC"
              },
              { "name": "org.gradle.caching", "value": "true" },
              { "name": "org.gradle.parallel", "value": "true" },
              { "name": "org.gradle.configuration-cache", "value": "true" },
              { "name": "android.enableJetifier", "value": "false" },
              { "name": "newArchEnabled", "value": "true" },
              { "name": "hermesEnabled", "value": "true" },
              { "name": "EX_DEV_CLIENT_NETWORK_INSPECTOR", "value": "true" }
            ]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "apiBaseUrl": "http://172.20.168.21:8000",
      "useMockApi": false,
      "router": {},
      "eas": {
        "projectId": "cfd43756-f2e8-4971-82e0-8cd822010cfe"
      }
    }
  }
}
